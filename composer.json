{
  "name": "pubpeer-foundation/publication-data-extractor",
  "description": "Extract publication data from multiple external APIs",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Xavier Roussel",
      "email": "xavier.roussel@pubpeer.org"
    }
  ],
  "require": {
    "php": "^7.1|^8.0|^8.2",
    "graham-campbell/guzzle-factory": "^7.0",
    "nesbot/carbon": "^2.68.1",
    "serkin/volan": "dev-master",
    "tightenco/collect": "^9.52",
    "symfony/dom-crawler": "^5.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0"
  },
  "autoload": {
    "files": [
      "src/Support/helpers.php"
    ],
    "psr-4": {
      "PubPeerFoundation\\PublicationDataExtractor\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PubPeerFoundation\\PublicationDataExtractor\\Test\\": "tests/"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  }
}
